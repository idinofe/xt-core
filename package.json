{
  "name": "@dinofe/xt-core",
  "version": "0.0.12",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idinotes/xt-core.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "@types/koa": "^2.13.6",
    "@types/koa-router": "^7.4.4",
    "@types/qs": "^6.9.7",
    "@vitest/ui": "^0.32.2",
    "father": "^4.2.3",
    "husky": "^7.0.1",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5",
    "vitest": "^0.32.2"
  },
  "scripts": {
    "prepare": "husky install",
    "build:types": "npx tsc --declaration --outDir types --emitDeclarationOnly -p tsconfig.build.json",
    "build": "npm run build:types && father build",
    "test": "node test/index.test.js",
    "vitest": "vitest",
    "vitest:run": "vitest run",
    "vitest:ui": "vitest --ui"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./types/*.d.ts",
      "import": "./dist/esm/*.js",
      "default": "./dist/cjs/*.js"
    },
    "./storage/*": {
      "types": "./types/storage/*.d.ts",
      "import": "./dist/esm/storage/*.js",
      "default": "./dist/cjs/storage/*.js"
    }
  },
  "types": "./types/index.d.ts",
  "module": "./dist/esm/index",
  "main": "./dist/cjs/index",
  "unpkg": "./dist/umd/xt-core.min.js",
  "jsdelivr": "./dist/umd/xt-core.min.js",
  "dependencies": {
    "@types/big.js": "^6.1.6",
    "apisauce": "^3.0.1",
    "big.js": "^6.2.1",
    "dexie": "^3.0.3",
    "qs": "^6.11.2"
  },
  "optionalDependencies": {
    "esbuild-android-arm64": "^0.15.18"
  }
}
